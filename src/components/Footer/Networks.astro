---
import { networks } from '../../data/networks'
---

<div class="flex mt-4 sm:justify-center sm:mt-0">
    {
        networks.map(network => (
            <a href={network.href} class="text-gray-500 hover:text-white ms-5 transition-colors duration-300 ease">
                <network.icon class="w-5 h-5"/>
                <span class="sr-only">{network.alt}</span>
            </a>
        ))
    }
</div>